{
  "name": "@shipengine/integration-platform-cli",
  "description": "A CLI tool for working with your ShipEngine Integration account.",
  "version": "0.0.14",
  "author": {
    "name": "ShipEngine",
    "email": "support@shipengine.com",
    "url": "https://www.shipengine.com"
  },
  "bin": {
    "shipengine": "./bin/run"
  },
  "bugs": "https://github.com/ShipEngine/shipengine-integration-platform-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.7.0",
    "@oclif/config": "^1.16.0",
    "@oclif/fixpack": "^2.3.0",
    "@oclif/plugin-autocomplete": "^0.2.0",
    "@oclif/plugin-help": "^3.1.0",
    "@oclif/plugin-not-found": "^1.2.4",
    "@shipengine/capitalization": "^1.2.0",
    "@shipengine/integration-platform-loader": "0.0.32",
    "@shipengine/integration-platform-sdk": "0.0.34",
    "axios": "^0.19.2",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "escape-string-regexp": "^4.0.0",
    "form-data": "^3.0.0",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.0",
    "joi": "^14.3.1",
    "js-yaml": "^3.14.0",
    "json5": "^2.1.3",
    "lodash": "^4.17.19",
    "luxon": "^1.24.1",
    "mocha": "^7.1.2",
    "netrc": "^0.1.4",
    "nps": "^5.9.12",
    "object-hash": "^2.0.3",
    "p-retry": "^4.2.0",
    "p-timeout": "^3.2.0",
    "recursive-readdir": "^2.2.2",
    "sort-pjson": "^1.0.3",
    "tslib": "^2.0.0",
    "update-notifier": "^4.1.0",
    "uuid": "^8.1.0",
    "yeoman-environment": "^2.10.3",
    "yeoman-generator": "^4.10.1"
  },
  "devDependencies": {
    "@jsdevtools/eslint-config-modular": "^8.0.3",
    "@jsdevtools/version-bump-prompt": "^6.0.2",
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.6",
    "@types/chai": "^4.1.7",
    "@types/form-data": "^2.5.0",
    "@types/fs-extra": "^9.0.1",
    "@types/hapi__joi": "^17.1.2",
    "@types/inquirer": "^6.5.0",
    "@types/joi": "^14.3.4",
    "@types/js-yaml": "^3.12.4",
    "@types/json5": "0.0.30",
    "@types/luxon": "^1.24.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.20",
    "@types/recursive-readdir": "^2.2.0",
    "@types/update-notifier": "^4.1.0",
    "@types/uuid": "^8.0.0",
    "@types/yeoman-environment": "^2.3.3",
    "@types/yeoman-generator": "^3.1.4",
    "@types/yeoman-test": "^2.0.4",
    "chai-as-promised": "^7.1.1",
    "chai-fs": "^2.0.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "eslint-config-prettier": "^6.11.0",
    "globby": "^10.0.2",
    "nock": "^12.0.3",
    "npm-check": "^5.9.0",
    "nyc": "^15.1.0",
    "shx": "^0.3.2",
    "sinon": "^9.0.2",
    "source-map-support": "^0.5.17",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.6.0"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=10.1.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/templates"
  ],
  "homepage": "https://www.shipengine.com/docs/",
  "keywords": [
    "api",
    "cli",
    "integrations",
    "oclif",
    "shipengine"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "shipengine",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ],
    "hooks": {
      "init": "./lib/hooks/init"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ShipEngine/shipengine-cli.git"
  },
  "scripts": {
    "build": "tsc",
    "bump": "bump --tag --push --all",
    "clean": "shx rm -rf .nyc_output coverage lib",
    "coverage": "nyc npm run test",
    "lint": "eslint . --ext .ts --config .eslintrc.js",
    "mocha": "mocha",
    "postpack": "shx rm -rf oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc.js",
    "prepack": "shx rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "release": "npm run upgrade && npm run clean && npm test && npm run bump",
    "test": "shx rm -rf lib && npm run build && nyc --extension .ts mocha --forbid-only && npm run lint",
    "upgrade": "npm-check -u && npm audit fix",
    "version": "oclif-dev readme && git add README.md",
    "watch": "tsc --watch"
  },
  "types": "lib/index.d.ts"
}
